{
	"name": "wj-plugin",
	"displayName": "wj-plugin",
	"description": "vs plugin",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloVX",
		"onCommand:wjWeb.start",
		"onCommand:extension.vx.getCurrentFilePath"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloVX",
				"title": "Hello VX"
			},
			{
				"command": "wjWeb.start",
				"title": "Start web session",
				"category": "Wj Web"
			},
			{
				"command": "extension.vx.getCurrentFilePath",
				"title": "获取当前文件(夹)路径"
			}
		],
		"keybindings": [
			{
				"command": "extension.helloVX",
				"key": "ctrl+f10",
				"mac": "cmd+f10",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.vx.getCurrentFilePath",
					"group": "navigation"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "wj-vx",
					"title": "wj",
					"icon": "images/v.svg"
				}
			]
		},
		"views": {
			"wj-vx": [
				{
					"id": "wj-vx-view",
					"name": "wj-vx-view"
				}
			]
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}